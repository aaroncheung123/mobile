{
  "expo": {
    "sdkVersion": "27.0.0",
  	"name": "Venture Pass",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "description": "Venture Pass customers can access their passes right from their phone",
    "icon": "./assets/images/branding/icon.png",
    "ios": {
      "bundleIdentifier": "com.myventurepass.venturepass", 
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "eliteworks.com": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.1",
              "NSIncludesSubdomains": true
            }
          }
        }
      },
      "buildNumber": "1.0"
    },
    "android": {
      "versionCode": 10000,
      "package": "com.myventurepass.venturepass",
      "permissions": [
        "INTERNET"
      ],
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  },
  "packagerOpts": {
    "projectRoots": "",
    "config": "rn-cli.config.js"
  }
}



