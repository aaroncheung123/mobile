  {
  "expo": {
    "sdkVersion": "27.0.0",
    "name": "EliteWorks WorkForce",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "description": "EliteWorks WorkForce allows employees to fulfill work orders directly from their phone",
    "icon": "./assets/images/branding/icon.png",
    "ios": {
      "bundleIdentifier": "eliteworks.workforce",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "eliteworks.com": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.1",
              "NSIncludesSubdomains": true
            }
          }
        }//,
        /*"NSCameraUsageDescription": "EliteWorks would like access to use your camera",
        "NSPhotoLibraryUsageDescription": "EliteWorks would like access to use your camera roll"*/
      },
      "buildNumber": "1.0"
    },
    "android": {
      "versionCode": 10000,
      "package": "eliteworks.workforce",
      "permissions": [
        "INTERNET",
        "CAMERA"
      ],
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  },
  "packagerOpts": {
  	"projectRoots": "",
  	"config": "rn-cli.config.js"
  }
}
