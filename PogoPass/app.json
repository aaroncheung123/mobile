{
  "expo": {
    "name": "Pogo Pass",
    "description": "View Pogo Pass directly from your phone",
    "slug": "PogoPassNew",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "3.2",
    "orientation": "portrait",
    "icon": "./assets/images/icons/icon-ios.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "pogopass.mobile.eliteworks",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "pogopass.com": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.1",
              "NSIncludesSubdomains": true
            },
            "eliteworks.com": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.1",
              "NSIncludesSubdomains": true
            },
            "amazonaws.com": {
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.1",
              "NSIncludesSubdomains": true
            }
          }
        }
      },
      "buildNumber": "3.0.4"
    },
    "android": {
      "versionCode": 25000,
      "package": "eliteworks.pogopass.mobile",
      "permissions": [
        "INTERNET"
      ],
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  },
  "packagerOpts": {
    "projectRoots": "",
    "config": "./metro.config.js"
  }
}
